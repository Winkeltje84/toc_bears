<h3>Public Events</h3>
<table class="table table-striped">
  <thead>
    <tr>
      <th>#</th>
      <th>Title</th>
      <th>Location</th>
      <th>Spaces Available</th>
      <th>Date</th>
    </tr>
  </thead>
  <tbody>
    <% @public_events.each do |event| %>
      <tr>
        <td><%= event.id %></td>
        <td><%= event.title %></td>
        <td><%= event.location %></td>
        <td><%= event.spaces_available %></td>
        <td><%= event.date %></td>
      </tr>
    <% end %>
  </tbody>
</table>
